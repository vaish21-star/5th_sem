import pandas as pd
import numpy as np
data = pd.DataFrame([[9, 4, 8, 9],
 [6, 10, 7, 6],
 [7, 6, 8, 5]],
 columns = ['Maths', 'English', 'Science', 'History'])
print(data)
print(data.agg(['sum', 'min', 'max']))
m = lambda x: x + 10
print("lambda:",m(5))
a = list(map(lambda x: x * x, data['Maths']))
print("map:",a)
b = list(filter(lambda x: x % 2 == 0, data['Maths']))
print("filter:",b)
from functools import reduce
c = reduce(lambda x, y: x + y, data['Science'])
print(b)
